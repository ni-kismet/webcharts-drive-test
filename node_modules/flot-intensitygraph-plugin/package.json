{
  "name": "flot-intensitygraph-plugin",
  "version": "1.0.1",
  "description": "Flot intensity graph plugin",
  "main": "jquery.flot.intensitygraph.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "node node_modules/karma/bin/karma start --single-run --concurrency=1",
    "coverage": "node node_modules/karma/bin/karma start --single-run --coverage --browsers=Chrome",
