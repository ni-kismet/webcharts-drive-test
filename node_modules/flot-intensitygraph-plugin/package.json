    "url": "https://github.com/ni-kismet/flot-intensitygraph-plugin/issues"
  },
  "homepage": "https://github.com/ni-kismet/flot-intensitygraph-plugin",
  "devDependencies": {
    "engineering-flot": "^1.1.13",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-safari-launcher": "^1.0.0",
